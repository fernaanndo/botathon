  <stage stageid="2e67a2b9-10bf-4d4d-9bd9-db68edaf7662" name="Release Instance" type="Code">
    <subsheetid>4fb11e41-83d8-461d-8c0d-04cdd2f670e2</subsheetid>
    <loginhibit />
    <display x="15" y="-90" w="120" h="30" />
    <inputs>
      <input type="number" name="handle" expr="[handle]" />
    </inputs>
    <onsuccess>dc556131-4dde-459f-9af8-8c854fcae760</onsuccess>
    <code><![CDATA[
	Dim current as Object = CurrentInstance
	Dim inst as Object = GetInstance(handle) ' Careful - sets the 'current instance' to inst

	HandleMap.Remove(handle)
	InstanceMap.Remove(inst)

	If Object.ReferenceEquals(inst, current) Then
		CurrentInstance = Nothing
	Else
		CurrentInstance = current
	End If
]]></code>
  </stage>
