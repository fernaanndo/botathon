    <loginhibit />
    <display x="-15" y="15" />
    <font family="Tahoma" size="10" style="Regular" color="000000" />
    <inputs>
      <input type="number" name="Handle" narrative="An integer value indicating the Excel instance that contains the specific workbook." stage="Handle" />
