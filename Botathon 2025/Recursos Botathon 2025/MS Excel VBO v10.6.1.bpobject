  <stage stageid="8b526d9f-c7bd-4190-8c33-a6f5ad7982fa" name="SaveWorkbookAs" type="Code">
    <subsheetid>21a53e34-8128-41cb-ad73-17250758ff23</subsheetid>
    <loginhibit />
    <display x="15" y="-15" w="120" h="30" />
    <inputs>
      <input type="number" name="handle" expr="[handle]" />
      <input type="text" name="workbookname" expr="[workbookname]" />
      <input type="text" name="filename" expr="[filename]" />
      <input type="number" name="Timeout" expr="[Timeout]" />
      <input type="text" name="password" expr="[password]" />
      <input type="number" name="fileFormat" expr="[File Format]" />
    </inputs>
    <outputs>
      <output type="text" name="newworkbookname" stage="newworkbookname" />
    </outputs>
    <onsuccess>b4f79caf-dea6-4a9a-95f0-0c05bea8b485</onsuccess>
    <code><![CDATA[newworkbookname = ExecWithTimeout(Timeout, "Save Workbook As",
Function()
    Dim wb as Object  = GetWorkbook(handle,workbookname)
    Dim excel as Object = wb.Application

    excel.DisplayAlerts = False

	' Set the password value. If no password was supplied as input, no password will take effect.
	wb.Password = password

    wb.SaveAs(filename, fileFormat)
    excel.DisplayAlerts = True

    Return wb.Name
End Function)]]></code>
