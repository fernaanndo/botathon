  <stage stageid="f308d1c5-88b0-455a-bff7-8163c95d1eab" name="Open Workbook" type="Code">
    <subsheetid>2fb01e3f-e275-43bc-83eb-579001da335b</subsheetid>
    <loginhibit />
    <display x="15" y="15" w="120" h="30" />
    <inputs>
      <input type="text" name="filename" expr="[filename]" />
      <input type="number" name="handle" expr="[handle]" />
      <input type="number" name="timeout" expr="[Timeout]" />
      <input type="text" name="password" expr="[password]" />
      <input type="flag" name="updateLinks" expr="[Update Links]" />
    </inputs>
    <outputs>
      <output type="text" name="name" stage="workbookname" />
    </outputs>
    <onsuccess>6c2fa22d-1876-46b7-92eb-cf8343d0911f</onsuccess>
    <code><![CDATA[Dim update As Integer = 0

If (updateLinks) Then
	update = 3
End If

name = ExecWithTimeout(Timeout, "Open Workbook",
Function()
    Dim instance = GetInstance(handle)
    Dim workbooks As object = GetProperty(instance, "Workbooks")
    Dim wb As Object = Invoke(workbooks, "Open", filename, update, False, vbNull, password)
    Invoke(wb, "Activate")
    Return wb.Name
End Function)]]></code>
  </stage>
