  <stage stageid="5c9a9a9a-4628-4acd-95bc-c7b2472de164" name="Copy Chart to File" type="Code">
    <subsheetid>9c95435d-a34c-46cc-b90d-ab2ae7a3ec91</subsheetid>
    <loginhibit />
    <display x="15" y="90" w="150" h="30" />
    <font family="Tahoma" size="10" style="Regular" color="000000" />
    <inputs>
      <input type="number" name="Handle" expr="[handle]" />
      <input type="text" name="Source Workbook" expr="[Source Workbook]" />
      <input type="text" name="Source Worksheet" expr="[Source Worksheet]" />
      <input type="text" name="Chart Name" expr="[Chart Name]" />
      <input type="text" name="Destination File" expr="[Destination File]" />
    </inputs>
